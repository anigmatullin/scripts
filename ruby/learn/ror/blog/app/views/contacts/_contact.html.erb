<div id="<%= dom_id contact %>" class="card" style="width: 18rem;">

  <table class="table">

    <tr>
      <td>Name:</td>
      <td><%= contact.name %></td>
    </tr>

    <tr>
      <td>Lastname:</td>
      <td><%= contact.lastname %></td>
    </tr>

    <tr>
      <td>Email:</td>
      <td><%= contact.email %></td>
    </tr>

    <tr>
      <td>Phone:</td>
      <td><%= contact.phone %></td>
    </tr>

    <tr>
      <td>Org:</td>
      <td><%= contact.org %></td>
    </tr>

  </table>

</div>
